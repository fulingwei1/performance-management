# 服务器配置
PORT=3001
NODE_ENV=development

# 数据库配置（必填，无 MySQL 时服务将退出）
DB_HOST=localhost
DB_PORT=3306
DB_NAME=performance_db
DB_USER=root
DB_PASSWORD=your_password

# 可选：读缓存（ENABLE_CACHE=true 时启用，CACHE_TTL_MS 默认 5 分钟）
# ENABLE_CACHE=false
# CACHE_TTL_MS=300000

# JWT配置
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 初始化管理员密码
ADMIN_DEFAULT_PASSWORD=admin123
