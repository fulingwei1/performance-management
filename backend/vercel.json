{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dist/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_MEMORY_DB": "true"
  }
}